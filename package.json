{
  "name": "discord-oauth-proxy-auth",
  "version": "1.0.0",
  "description": "Discord OAuth proxy authentication provider for nginx with role-based access control and rate limiting",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "restart": "npm run pm2:restart",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop discord-oauth-proxy",
    "pm2:restart": "pm2 restart discord-oauth-proxy",
    "pm2:logs": "pm2 logs discord-oauth-proxy",
    "pm2:status": "pm2 status",
    "pm2:delete": "pm2 delete discord-oauth-proxy"
  },
  "dependencies": {
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "passport": "^0.6.0",
    "passport-discord": "^0.1.4",
    "axios": "^1.5.0",
    "dotenv": "^16.3.1",
    "connect-redis": "^7.1.0",
    "redis": "^4.6.8",
    "session-file-store": "^1.5.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "pm2": "^5.3.0"
  },
  "keywords": [
    "discord",
    "oauth",
    "nginx",
    "proxy-auth",
    "authentication",
    "authorization",
    "rate-limiting",
    "guild",
    "role-based-access",
    "express",
    "passport"
  ],
  "author": "shigbeard <shigbeard@shigbeard.xyz> (https://ozfortress.com)",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/discord-oauth-proxy-auth.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/discord-oauth-proxy-auth/issues"
  },
  "homepage": "https://github.com/yourusername/discord-oauth-proxy-auth#readme"
}
